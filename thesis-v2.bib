  file = {C\:\\Users\\Amy\\Zotero\\storage\\LKV7JL5V\\Ebert_Wegner_2010_Time warp.pdf;C\:\\Users\\Amy\\Zotero\\storage\\MTY66829\\Ebert and Wegner - 2010 - Time warp Authorship shapes the perceived timing .pdf}
