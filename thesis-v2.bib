@article{desoaresTopdownAttentionShifts2024,
  title = {Top-down Attention Shifts Behavioral and Neural Event Boundaries in Narratives with Overlapping Event Scripts},
  author = {De Soares, Alexandra and Kim, Tony and Mugisho, Franck and Zhu, Elen and Lin, Allison and Zheng, Chen and Baldassano, Christopher},
  year = {2024},
  month = oct,
  journal = {Current Biology},
  volume = {34},
  number = {20},
  pages = {4729-4742.e5},
  issn = {09609822},
  doi = {10.1016/j.cub.2024.09.013},
  urldate = {2025-04-11},
  abstract = {Understanding and remembering the complex experiences of everyday life relies critically on prior schematic knowledge about how events in our world unfold over time. How does the brain construct event representations from a library of schematic scripts, and how does activating a specific script impact the way that events are segmented in time? We developed a novel set of 16 audio narratives, each of which combines one of four location-relevant event scripts (restaurant, airport, grocery store, and lecture hall) with one of four socially relevant event scripts (breakup, proposal, business deal, and meet cute), and presented them to participants in an fMRI study and a separate online study. Responses in the angular gyrus, parahippocampal gyrus, and subregions of the medial prefrontal cortex (mPFC) were driven by scripts related to both location and social information, showing that these regions can track schematic sequences from multiple domains. For some stories, participants were primed to attend to one of the two scripts by training them to listen for and remember specific script-relevant episodic details. Activating a location-related event script shifted the timing of subjective event boundaries to align with script-relevant changes in the narratives, and this behavioral shift was mirrored in the timing of neural responses, with mPFC event boundaries (identified using a hidden Markov model) aligning to location-relevant rather than socially relevant boundaries when participants were location primed. Our findings demonstrate that neural event dynamics are actively modulated by top-down goals and provide new insight into how narrative event representations are constructed through the activation of temporally structured prior knowledge.},
  langid = {english},
  lccn = {1},
  keywords = {/unread},
  file = {C:\Users\Amy\Zotero\storage\H7MTCFYT\De Soares et al. - 2024 - Top-down attention shifts behavioral and neural ev.pdf}
}

@article{ebertTimeWarpAuthorship2010,
  title = {Time Warp: {{Authorship}} Shapes the Perceived Timing of Actions and Events},
  shorttitle = {Time Warp},
  author = {Ebert, Jeffrey P. and Wegner, Daniel M.},
  year = {2010},
  month = mar,
  journal = {Consciousness and Cognition},
  volume = {19},
